.container {
  height: 100%;
  overflow-y: auto;
  background-color: #f2f2f2;
}
.stand_key {
  display: flex;
  justify-content: center;
  flex-direction: column;
  margin: 60rpx 0 24rpx 0;
  padding: 0 48rpx;
}
.winds, .hours {
  display: flex;
  justify-content: space-between;
}
.winds .item, .hours .item {
  width: 120rpx;
  height: 120rpx;
  background-color: #fff;
  line-height: 120rpx;
  border-radius: 100%;
  text-align: center;
}
.winds .item:active, .hours .item:active{
  background-color: rgba(0,0,0,.1);
}
.light {
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 84rpx 0;
}
.img {
  width: 160rpx;
  height: 160rpx;
  background-color: #fff;
  border-radius: 16rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.img:active {
  background-color: rgba(0,0,0,.1);
}
.img image {
  width: 104rpx;
  height: 104rpx;
}