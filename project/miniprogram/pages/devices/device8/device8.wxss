.container {
  height: 100%;
  box-sizing: border-box;
  background-color: #f2f2f2;
}
.normalKey {
  width: 100%;
}
.on-off {
  padding: 48rpx 120rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.on, .off {
  width: 160rpx;
  height: 160rpx;
  background-color: #fff;
  border-radius: 50%;
  text-align: center;
  font-size: 40rpx;
  line-height: 160rpx;
}
.on:active, .off:active {
  background-color: rgba(0,0,0,.1);
}
.switch {
  display: flex;
  justify-content: center;
  align-items: center;
}
.sw-icon {
  width: 160rpx;
  height: 160rpx;
  background-color: #fff;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.sw-icon:active {
  background-color: rgba(0,0,0,.1);
}
.sw-icon image {
  width: 80rpx;
  height: 80rpx;
}