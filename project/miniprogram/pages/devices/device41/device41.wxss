.container {
  height: 100%;
  overflow-y: auto;
  background-color: #f2f2f2;
}
.stand_key {
  display: flex;
  justify-content: center;
  flex-direction: column;
  margin: 60rpx 0 24rpx 0;
  padding: 0 48rpx;
}
.winds {
  display: flex;
  justify-content: space-between;
}
.winds .item {
  width: 120rpx;
  height: 120rpx;
  background-color: #fff;
  line-height: 120rpx;
  border-radius: 100%;
  text-align: center;
}
.winds .item:active {
  background-color: rgba(0,0,0,.1);
}
.timer {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 120rpx;
  border-radius: 60rpx;
  background-color: #fff;
  margin: 60rpx  0;
}
.btn-reduce, .btn-plus {
  margin-left: 20rpx;
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
}
.btn-reduce:active, .btn-plus:active {
  background-color: rgba(0,0,0,.1);
}
.btn-plus {
  margin-right: 20rpx;
}
.funs {
  display: flex;
  justify-content: space-between;
}
.item2 {
  width: 160rpx;
  height: 160rpx;
  background-color: #fff;
  border-radius: 16rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.item2:active {
  background-color: rgba(0,0,0,.1);
}
.item2 image {
  width: 104rpx;
  height: 104rpx;
}